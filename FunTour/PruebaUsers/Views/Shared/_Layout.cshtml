<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>FunTour</title>
    @*@Scripts.Render("~/bundles/modernizr")*@

    <!-- JQuery Library References -->
    <script src="@Url.Content("~/Scripts/jquery-1.10.2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.10.3.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-common.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-validate.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-validate.unobtrusive.js")" type="text/javascript"></script>

        <!--Import Google Icon Font-->
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css?family=Pacifico" rel="stylesheet">
        <!--Import materialize.css-->
        <link rel="stylesheet" href="css/main.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/css/materialize.min.css">
        <script defer src="https://use.fontawesome.com/releases/v5.0.13/js/all.js" integrity="sha384-xymdQtn1n3lH2wcu0qhcdaOpQwyoarkgLVxC/wZ5q7h9gHtxICrpcaSUfygqZGOe" crossorigin="anonymous"></script>
        <!--Let browser know website is optimized for mobile-->
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>FanTour-Agencia de Turismo </title>

    <!-- jQuery UI Theme -->
    <link href="@Url.Content("~/Content/themes/ui-lightness/jquery-ui-1.10.3.min.css")" rel="stylesheet" type="text/css" />
</head>

    <body>

        <!--S Navbar -->
        <div class="navbar-fixed general">
            <Nav class="amber darken-1">
                <div class="container">
                    <div class="nav-wrapper">
                        <a href="~/Home/Index" class="brand-logo purple-text text-darken-1 left" style="font-family:'Pacifico',cursive;"><i class="material-icons " size=20px>star_half</i>  FantasticTour</a>
                        <a href="#" data-target="mobile-nav"
                           class="sidenav-trigger" align="left">
                            <i class="material-icons large">menu</i>
                        </a>
                        <ul class="right hide-on-med-and-down">
                            <li>
                                <a href="#rusia">Rusia 2018</a>
                            </li>
                            <li>
                                <a href="#vacaciones">Vacaciones</a>
                            </li>
                            <li>
                                <a href="#Egresados">Egresados</a>
                            </li>
                            <li>
                                <a href="#contactanos">Contactanos</a>
                            </li>
                            <li>
                                <a href="~/Account/Login">Ingresar</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </Nav>
        </div>
        <ul class="sidenav" id="mobile-nav">
            <li>
                <a href="#rusia">Rusia 2018</a>
            </li>
            <li>
                <a href="#vacaciones">Vacaciones</a>
            </li>
            <li>
                <a href="#Egresados">Egresados</a>
            </li>
            <li>
                <a href="#contactanos">Contáctanos</a>
            </li>
            <li>
                <a href="~/Account/Login">Ingresar</a>
            </li>

        </ul>


    @RenderBody()


        <!-- Section: FollowUs -->
        <section class="section section-follow red darken-1 white-text center">
            <div class="container">
                <div class="row">
                    <div class="col s12">
                        <h5>Sígue a FanTour</h5>
                        <p>Síguenos en las redes sociales y accede a promociones!</p>
                        <a href="#" class="white-text">
                            <i class="fab fa-facebook fa-4x"></i>
                        </a>
                        <a href="#" class="white-text">
                            <i class="fab fa-twitter fa-4x"></i>
                        </a>
                        <a href="#" class="white-text">
                            <i class="fab fa-instagram fa-4x"></i>
                        </a>
                        <a href="#" class="white-text">
                            <i class="fab fa-google-plus fa-4x"></i>
                        </a>
                        <a href="#" class="white-text">
                            <i class="fab fa-pinterest fa-4x"></i>
                        </a>


                    </div>
                </div>
            </div>
        </section>

        <!--Footer  -->
        <footer class="section amber darken-1 white-text center">
            <p class="flow-text">FantasticTour &copy; 2018</p>
        </footer>

        <environment>
            <script src="~/Scripts/jquery-1.10.2.min.js" asp-append-version="true"></script>
            <script src="~/Scripts/bootstrap.min.js" asp-append-version="true"></script>
            <script type="text/javascript" src="js/bootstrap.js" asp-append-version="true"></script>
            <script type="text/javascript" src="js/bootstrap.bundle.js" asp-append-version="true"></script>
        </environment>

    </body>
</html>
