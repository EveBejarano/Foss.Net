@using System.Web.Optimization
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>FunTour</title>
    @Scripts.Render("~/bundles/modernizr")

    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Pacifico" rel="stylesheet">
    <!--Import materialize.css-->
    <link rel="stylesheet" href="~/Content/css/main.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/css/materialize.min.css">
    <script defer src="https://use.fontawesome.com/releases/v5.0.13/js/all.js" integrity="sha384-xymdQtn1n3lH2wcu0qhcdaOpQwyoarkgLVxC/wZ5q7h9gHtxICrpcaSUfygqZGOe" crossorigin="anonymous"></script>
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>FanTour-Agencia de Turismo </title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    @Scripts.Render("~/bundles/modernizr")

    <!-- JQuery Library References -->
    <script src="@Url.Content("~/Scripts/jquery-1.10.2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.10.3.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-common.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Site.js")" type="text/javascript"></script>

    <!-- jQuery UI Theme -->
    <link href="@Url.Content("~/Content/themes/ui-lightness/jquery-ui-1.10.3.min.css")" rel="stylesheet" type="text/css" />

</head>
<body>
    <main>
        <!--S Navbar -->
        <div class="navbar-fixed general">
            <Nav class="light-blue darken-4">
                <div class="container">
                    <div class="nav-wrapper">
                        <a href="~/Admin/Index" class="brand-logo white-text text-darken-1 left" style="font-family:'Pacifico',cursive;"><i class="material-icons " size=20px>star_half</i>  FantasticTour</a>
                        <a href="#" data-target="mobile-nav"
                           class="sidenav-trigger" align="left">
                            <i class="material-icons large">menu</i>
                        </a>
                        <ul class="right hide-on-med-and-down">
                            <li>
                                <a href="~/Admin/RoleIndex">Roles</a>
                            </li>
                            <li>
                                <a href="~/Admin/PermissionIndex">Permisos</a>
                            </li>
                            <li>
                                <a href="~/TravelPackages/Index">Paquetes</a>
                            </li>
                            <li>
                                <a href="">Newsletters</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </Nav>
        </div>
        <ul class="sidenav" id="mobile-nav">
            <li>
                <a href="~/Views/Admin/RoleIndex.cshtml">Roles</a>
            </li>
            <li>
                <a href="~/Views/Admin/PermissionIndex.cshtml">Permisos</a>
            </li>
            <li>
                <a href="~/Views/TravelPackages/Index.cshtml">Paquetes</a>
            </li>
            <li>
                <a href="">Newsletters</a>
            </li>
        </ul>

        @RenderBody()
    </main>
        <!--Footer  -->
        <footer class="page-footer light-blue darken-4 white-text center">
            <div class="footer-copyright">
                <div class="container">
                    © 2018 FantasticTour
                </div>
            </div>
        </footer>

</body>
</html>

