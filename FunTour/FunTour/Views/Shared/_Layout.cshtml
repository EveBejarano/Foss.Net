@using System.Web.Optimization
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>FunTour</title>
    @Scripts.Render("~/bundles/modernizr")

    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Pacifico" rel="stylesheet">
    <!--Import materialize.css-->
    <link rel="stylesheet" href="~/Content/css/main.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-beta/css/materialize.min.css">
    <script defer src="https://use.fontawesome.com/releases/v5.0.13/js/all.js" integrity="sha384-xymdQtn1n3lH2wcu0qhcdaOpQwyoarkgLVxC/wZ5q7h9gHtxICrpcaSUfygqZGOe" crossorigin="anonymous"></script>
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>FanTour-Agencia de Turismo </title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    @Scripts.Render("~/bundles/modernizr")

    <!-- JQuery Library References -->
    <script src="@Url.Content("~/Scripts/jquery-1.10.2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.10.3.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-common.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Site.js")" type="text/javascript"></script>

    <!-- jQuery UI Theme -->
    <link href="@Url.Content("~/Content/themes/ui-lightness/jquery-ui-1.10.3.min.css")" rel="stylesheet" type="text/css" />

</head>
<body>
    <!--S Navbar -->
    <div class="navbar-fixed general">
        <Nav class="light-blue darken-1">
            <div class="container">
                <div class="nav-wrapper">
                    <a href="~/Home/Index" class="brand-logo white-text text-darken-1 left" style="font-family:'Pacifico',cursive;"><i class="material-icons " size=20px>star_half</i>  FantasticTour</a>
                    <a href="#" data-target="mobile-nav"
                       class="sidenav-trigger" align="left">
                        <i class="material-icons large">menu</i>
                    </a>

                    @Html.Partial("_LoginPartial")
                </div>
            </div>
        </Nav>
    </div>

    @RenderBody()

    <!-- Section: FollowUs -->
    <section class="section section-follow red darken-1 white-text center">
        <div class="container">
            <div class="row">
                <div class="col s12">
                    <h5>Seguí a FanTour</h5>
                    <p>¡Seguinos en las redes sociales y accedé a promociones!</p>
                    <a href="#" class="white-text">
                        <i class="fab fa-facebook fa-4x"></i>
                    </a>
                    <a href="#" class="white-text">
                        <i class="fab fa-twitter fa-4x"></i>
                    </a>
                    <a href="#" class="white-text">
                        <i class="fab fa-instagram fa-4x"></i>
                    </a>
                    <a href="#" class="white-text">
                        <i class="fab fa-google-plus fa-4x"></i>
                    </a>
                    <a href="#" class="white-text">
                        <i class="fab fa-pinterest fa-4x"></i>
                    </a>

                </div>
            </div>
        </div>
    </section>
    <!--Footer  -->
    <footer class="section light-blue darken-4 white-text center">
        <p class="flow-text">FantasticTour &copy; 2018</p>
    </footer>
</body>
</html>
