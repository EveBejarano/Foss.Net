@model FunTour.Models.TravelPackageViewModel
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@{
    ViewBag.Title = "AddServicesInPlace";
}


<h2>Agregar Servicios en @Model.ToPlace.Name:</h2>

<fieldset>
    <div>
        @Html.Partial("_AddHotelToTravelPackagePartialView", Model)
    </div>
    <br />

    <div>
        @Html.Partial("_AddEventToTravelPackagePartialView", Model)
    </div>

</fieldset>


@*<input type="button" id="AddServicesInPlaceReturn" value="Guardar y Continuar..." />*@
<button class="waves-effect waves-light btn blue lighten-1" type="submit" onclick="AddServicesInPlace()">Guardar y Continuar</button>
@Html.ActionLink("Volver Atrás", "AddServicesToTravel", new { travelPackageViewModel = Model }, new { @class = "waves-effect waves-light btn blue lighten-1" })

<div>
    @Html.ActionLink("Volver al Menú", "Index", new { @class = "waves-effect waves-light btn blue lighten-1" })
</div>
